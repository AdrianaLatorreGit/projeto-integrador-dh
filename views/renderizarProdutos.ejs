<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Renderizar produtos</title>
    <link rel="stylesheet" href="../stylesheets/renderizarProdutos.css" />
  </head>
  <body>
    <div id="tabela">
     <div>
        <% produtos.forEach(produto => {%> <%= produto.id %> <%= produto.produto
        %> <%=produto.preco %> <%=produto.detalhes %> <%=produto.descricao %>
        <img
          id="imagemProduto"
          src="../uploads/uploads-produtos/<%=produto.image %>"
          alt=""
        />

        <% })%>
      </div>
    </div>
  </body>
</html>
